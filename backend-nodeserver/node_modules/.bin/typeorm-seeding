#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/nod" ]; then
  exec "$basedir/nod"  "$basedir/../typeorm-seeding/dist/cli.js" "$@"
else 
  exec nod  "$basedir/../typeorm-seeding/dist/cli.js" "$@"
fi
